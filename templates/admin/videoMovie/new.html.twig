{# templates/admin/new.html.twig #}
{% extends '@EasyAdmin/default/new.html.twig' %}

{% block entity_form %}
	{{ form_start(form) }}
	{{ form_errors(form) }}

	{{ form_row(form.title) }}
	{{ form_row(form.description) }}
	{{ form_row(form.released) }}
	{{ form_row(form.rating) }}
	{{ form_row(form.thumbnail) }}
	{{ form_row(form.runtime) }}
	{{ form_row(form.director) }}
	{{ form_row(form.actors) }}
	{{ form_row(form.imdbID) }}
	{{ form_row(form.videoUrl) }}
	{{ form_row(form._token) }}

	{% for file in encore_entry_js_files('app') %}
		<script defer src="{{ asset(file) }}"></script>
	{% endfor %}

	{{ include('admin/videoMovie/upload_frame.html.twig')}}

	{{ form_end(form) }}

{% endblock %}
